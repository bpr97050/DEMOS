/* Параметры для терминала 15ИЭ-00-0013 */

#define NLIN    24              /* Число строк */
#define SWID    80              /* Ширина экрана */
#define SYMSTRT "\033\105"      /* Стартовая последовательность */
#define SYMSTOP "\027"          /* Установка обычных параметров */
#define FRM     0               /* Курсор переходит за правый край экрана */
#define ROLL    0               /* Терминал работает со скроллингом */

/* Коды клавиатуры */

#define F_right "\031"          /* Вправо */
#define F_left  "\032"          /* Влево */
#define F_up    "\034"          /* Вверх */
#define F_down  "\035"          /* Вниз */
#define F_il    "\026"          /* Вставить строку */
#define F_dl    "\022"          /* Удалить строку */
#define F_ic    "\023"          /* Вставить символ */
#define F_dc    "\024"          /* Удалить символ */
#define F_del   "\177"          /* Забой */
#define F_tab   "\011"          /* Табуляция вперед */
#define F_btab  "\030"          /* Табуляция назад */
#define F_wtab  "\012\011"      /* Табуляция вперед по словам */
#define F_bwtab "\012\030"      /* Табуляция назад по словам */
#define F_exit  "\033\033"      /* Нормальный выход */
#define F_rew   "\001"          /* Перерисовка экрана */
#define F_na    "\020"          /* Ввод числового аргумента */
#define F_lnum  "\010\020"      /* Выдать номер текущей строки */
#define F_cnum  "\012\020"      /* Выдать номер текущей позиции в строке */
#define F_ret   "\025"          /* Установить в начало строки */
#define F_cr    "\015"          /* Возврат каретки */
#define F_aex   "\004\004\004\004" /* Аварийный выход */
#define F_inter "\003"          /* Прерывание */
#define F_flush "\027"          /* Отключение вывода на экран */
#define F_fsd   "\037"          /* Установка подстроки поиска */
#define F_find  "\014"          /* Поиск */
#define F_fsd2  "\010\037"      /* Второй шаблон поиска */
#define F_find2 "\010\014"      /* Поиск по второму шаблону */
#define F_savel "\005"          /* Запоминание строки в буфере */
#define F_incll "\007"          /* Вставка буфера в текст */
#define F_wrtex "\010\005"      /* Запись буфера в файл */
#define F_rdtex "\010\007"      /* Заполнение буфера из файла */
#define F_clrtx "\012\005"      /* Очистка буфера */
#define F_end   "\010\031"      /* В конец строки по непробелам */
#define F_beg   "\010\032"      /* В начало строки по непробелам */
#define F_pgup  "\010\034"      /* Страница вверх */
#define F_pgdown "\010\035"     /* Страница вниз */
#define F_shell "\010\002"      /* Вызов интерпретатора shell */
#define F_sins  "\010\023"      /* Режим вставки */
#define F_sdel  "\010\024"      /* Режим замены */
#define F_tof   "\010\015"      /* В начало файла */
#define F_eof   "\010\025"      /* В конец файла */
#define F_cat   "\002"          /* Склеивание/разрезание строк */
#define F_rpt   "\010\010"      /* Повторить пред. команду */
#define F_store "\010\033"      /* Запомнить изменения */
#define F_pmode "\010\022"      /* Редактирование программ */
#define F_tmode "\010\026"      /* Редактирование текстов */
#define F_parup "\012\034"      /* На параграф вверх */
#define F_pardown "\012\035"    /* На параграф вниз */
/* #define F_format  "\012\002"    * Форматирование абзаца */
#define F_mdef1 "\013\036"      /* Определить макро 1 */
#define F_mdef2 "\013\021"      /* Определить макро 2 */
#define F_mdef3 "\013\006"      /* Определить макро 3 */
#define F_m1    "\036"          /* Выполнить макро 1 */
#define F_m2    "\021"          /* Выполнить макро 2 */
#define F_m3    "\006"          /* Выполнить макро 3 */
#define FS_mdend "\013"         /* Символ конца определения макро */

#define Mname1  'Д'             /* Имя макро 1 */
#define Mname2  'М'             /* Имя макро 2 */
#define Mname3  'С'             /* Имя макро 3 */

/* Управляющие символы терминала */

#define S_right "\031"          /* Вправо курсор */
#define S_left  "\032"          /* Влево курсор */
#define S_up    "\034"          /* Вверх курсор */
#define S_down  "\035"          /* Вниз курсор */
#define S_ic    "\023"          /* Вставить символ */
#define S_dc    "\024"          /* Удалить символ */
#define S_ret   "\015"          /* Возврат каретки */
#define S_cr    "\012\015"      /* Возврат каретки/перевод строки */
#define S_home  "\010"          /* В начало экрана */
#define S_ru    "\022"          /* Роллирование вверх */
#define S_rd    "\026"          /* Роллирование вниз */
#define S_clrl  "\013"          /* Очистка строки (курсор в начале строки) */
#define S_clrel "\013"          /* Очистка конца строки */
#define S_erase "\010\014"      /* Очистка экрана */
#define S_bell  "\007"          /* Звонок */

#define S_pos   "\027\033Y  \033E"  /* Последовательность прямого */
#define POSX    3                   /* позиционирования */
#define POSY    4                   /*    см. pos.c     */
@


